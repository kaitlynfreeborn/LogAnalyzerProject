<Window x:Class="LogAnalysis.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LogAnalysis"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Width="790">
        <ComboBox x:Name="smb_select" HorizontalAlignment="Left" Margin="82,144,0,0" VerticalAlignment="Top" Width="364" SelectionChanged="ComboBox_SelectionChanged"/>
        <Label x:Name="lbl_loganalyzer" Content="Log Analyzer" HorizontalAlignment="Center" Margin="351,46,306,0" VerticalAlignment="Top" Height="39" Width="133" FontSize="20" FontFamily="Franklin Gothic Heavy" RenderTransformOrigin="0.514,0.474">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="0.44"/>
                    <RotateTransform/>
                    <TranslateTransform Y="-0.504"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label x:Name="lbl_select" Content="Select Event to Analyze" HorizontalAlignment="Left" Margin="82,113,0,0" VerticalAlignment="Top"/>
        <Calendar x:Name="cld_1" HorizontalAlignment="Left" Margin="538,144,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lbl_date" Content="Select Day to Analyze" HorizontalAlignment="Left" Margin="538,113,0,0" VerticalAlignment="Top" Width="179"/>
        <Button x:Name="btn_analyze" Content="Analyze" HorizontalAlignment="Left" Margin="290,244,0,0" VerticalAlignment="Top" Width="132" Height="46" FontSize="18" Click="Button_Click"/>

    </Grid>
</Window>
